{
  "name": "@primedao/contracts",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "truffle test",
    "lint": "solhint 'contracts/**/*.sol' && npx eslint test/*.js",
    "deploy:dao:rinkeby": "npx run scripts/migrateDAO.js --network rinkeby",
    "deploy:contracts:kovan": "truffle migrate --network kovan --reset",
    "deploy:dao:kovan": "npx run scripts/kovan/migrateDAO.js --network kovan",
    "setup:oracle:kovan": "npx truffle exec scripts/kovan/setupPriceOracle.js --network kovan",
    "setup:pool:create:kovan": "npx truffle exec scripts/kovan/setupPool.js --network kovan",
    "setup:pool:transfer:kovan": "npx truffle exec scripts/kovan/transferPool.js --network kovan",
    "setup:staking:innit:kovan": "npx truffle exec scripts/kovan/setupRewards.js --network kovan",
    "setup:staking:confirm:kovan": "npx truffle exec scripts/kovan/notifyRewards.js --network kovan",
    "coverage": "truffle run coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@daostack/arc": "0.0.1-rc.41",
    "@daostack/infra": "0.0.1-rc.17",
    "@daostack/migration": "0.0.1-rc.44-v0",
    "@openzeppelin/test-helpers": "^0.5.6",
    "@truffle/hdwallet-provider": "^1.1.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "fs": "0.0.1-security",
    "openzeppelin-solidity": "^2.4.0",
    "solhint": "^3.2.2",
    "truffle": "^5.1.43",
    "web3": "^1.3.0"
  },
  "devDependencies": {
    "eslint": "^7.9.0",
    "solidity-coverage": "^0.7.11"
  }
